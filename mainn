local Players = game:GetService("Players")
local player = Players.LocalPlayer

-- ScreenGui
local gui = Instance.new("ScreenGui")
gui.Parent = player:WaitForChild("PlayerGui")
gui.ResetOnSpawn = false


-- Main Frame
local frame = Instance.new("Frame")
frame.Parent = gui
frame.Size = UDim2.fromScale(0.25, 0.4)
frame.Position = UDim2.fromScale(0.35, 0.35)
frame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
frame.Active = true        -- REQUIRED for dragging
frame.Draggable = true    -- Makes it draggable

--67
local Name = Instance.new("TextLabel")
Name.Parent = frame
Name.Size = UDim2.fromScale(1, 0.08)
Name.Position = UDim2.fromScale(0, 0)
Name.BackgroundColor3 = Color3.fromRGB(255, 36, 0)
Name.Text = "private script-- minority of scripts are not mine-- reset to reset scripts"


-- ESPButton
local ESPbutton = Instance.new("TextButton")
ESPbutton.Parent = frame
ESPbutton.Size = UDim2.fromScale(0.2, 0.1)
ESPbutton.Position = UDim2.fromScale(0.1, 0.1)
ESPbutton.Text = "ESP-- R to toggle"
ESPbutton.BackgroundColor3 = Color3.fromRGB(255, 36, 0)
ESPbutton.TextColor3 = Color3.fromRGB(0, 0, 0)
-- ESPbutton click
ESPbutton.MouseButton1Click:Connect(function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/DND222dnD222/e/refs/heads/main/pesp"))()
end)

